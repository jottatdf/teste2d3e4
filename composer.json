{
    "name": "appwrite/server-ce",
    "description": "End to end backend server for frontend and mobile apps.",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Eldad Fux",
            "email": "eldad@appwrite.io"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/phpcs",
        "format": "vendor/bin/phpcbf"
    },
    "autoload": {
        "psr-4": {
            "Appwrite\\": "src/Appwrite",
            "Executor\\": "src/Executor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\E2E\\": "tests/e2e",
            "Tests\\Unit\\": "tests/unit",
            "Appwrite\\Tests\\": "tests/extensions"
        }
    },
    "require": {
        "php": ">=8.0.0",
        "ext-curl": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-dom": "*",
        "ext-redis": "*",
        "ext-swoole": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-zlib": "*",
        "ext-sockets": "*",
        "appwrite/php-runtimes": "0.13.*",
        "appwrite/php-clamav": "2.0.*",
        "utopia-php/abuse": "0.36.*",
        "utopia-php/analytics": "0.10.*",
        "utopia-php/audit": "0.38.*",
        "utopia-php/cache": "0.9.*",
        "utopia-php/cli": "0.15.*",
        "utopia-php/config": "0.2.*",
        "utopia-php/database": "0.48.*",
        "utopia-php/domains": "0.5.*",
        "utopia-php/dsn": "0.2.*",
        "utopia-php/framework": "0.33.*",
        "utopia-php/image": "0.6.*",
        "utopia-php/locale": "0.4.*",
        "utopia-php/logger": "0.3.*",
        "utopia-php/messaging": "0.9.*",
        "utopia-php/migration": "0.3.*",
        "utopia-php/orchestration": "0.9.*",
        "utopia-php/platform": "0.5.*",
        "utopia-php/pools": "0.4.*",
        "utopia-php/preloader": "0.2.*",
        "utopia-php/queue": "0.6.*",
        "utopia-php/registry": "0.5.*",
        "utopia-php/storage": "0.18.*",
        "utopia-php/swoole": "0.8.*",
        "utopia-php/vcs": "0.6.*",
        "utopia-php/websocket": "0.1.*",
        "matomo/device-detector": "6.2.1",
        "dragonmantank/cron-expression": "3.3.3",
        "phpmailer/phpmailer": "6.9.1",
        "chillerlan/php-qrcode": "5.0.1",
        "adhocore/jwt": "1.1.2",
        "spomky-labs/otphp": "11.2.0",
        "webonyx/graphql-php": "15.9.1",
        "league/csv": "9.14.0"
    },
    "repositories": [
        {
            "url": "https://github.com/appwrite/runtimes.git",
            "type": "git"
        }
    ],
    "require-dev": {
        "ext-fileinfo": "*",
        "appwrite/sdk-generator": "0.36.*",
        "phpunit/phpunit": "9.5.20",
        "squizlabs/php_codesniffer": "^3.7",
        "swoole/ide-helper": "5.0.2",
        "textalk/websocket": "1.5.7",
        "utopia-php/fetch": "0.1.*"
    },
    "provide": {
        "ext-phpiredis": "*"
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    }
}
